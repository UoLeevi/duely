{
  "name": "duely-email",
  "version": "0.0.9",
  "description": "Web server for sending email using gmail API.",
  "repository": "github:uoleevi/duely",
  "license": "MIT",
  "author": "Leevi Uotinen",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "cross-env-shell NODE_ENV='production' tailwindcss -i tailwind.css -c tailwind.config.js -o src/templates/partials/styles-css.hbs && webpack",
    "build+profile": "cross-env NODE_ENV='production' webpack --profile --json > stats.json",
    "build:css": "cross-env NODE_ENV='production' tailwindcss -i tailwind.css -c tailwind.config.js -o src/templates/partials/styles-css.hbs",
    "watch": "webpack --watch",
    "watch:css": "cross-env tailwindcss -w -i tailwind.css -c tailwind.config.js -o src/templates/partials/styles-css.hbs",
    "analyze": "npx webpack-bundle-analyzer stats.json"
  },
  "workspaces": [
    "../@duely/build",
    "../@duely/core",
    "../@duely/client",
    "../@duely/db",
    "../@duely/lambda",
    "../@duely/react"
  ],
  "dependencies": {
    "@duely/core": "0.4.26",
    "@duely/db": "0.4.17",
    "@duely/lambda": "0.4.0",
    "@types/validator": "^13.1.3",
    "axios": "^0.21.1",
    "base64url": "^3.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "hbs": "^4.1.2",
    "hbs-utils": "^0.0.4",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.6.3",
    "tailwindcss": "^2.2.7",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@duely/build": "0.4.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/hbs": "^4.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.37",
    "@types/nodemailer": "^6.4.4",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  }
}
