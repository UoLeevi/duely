{
  "name": "duely-ui-agency",
  "version": "0.6.20",
  "private": true,
  "scripts": {
    "build": "NODE_ENV='production' postcss node_modules/@duely/react/tailwind.css -o public/styles/index.css && react-scripts build",
    "build:css": "NODE_ENV='production' postcss node_modules/@duely/react/tailwind.css -o public/styles/index.css",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "watch:css": "postcss -w node_modules/@duely/react/tailwind.css -o public/styles/index.css"
  },
  "browserslist": {
    "production": [
      ">2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@duely/client": "0.0.75",
    "@duely/core": "0.0.22",
    "@duely/react": "^0.2.46",
    "@headlessui/react": "^0.2.0",
    "@tailwindcss/typography": "^0.3.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/react-dom": "^17.0.0",
    "graphql": "^15.2.0",
    "immer": "^8.0.0",
    "markdown-to-jsx": "^7.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.1",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@graphql-typed-document-node/core": "^3.1.0",
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "autoprefixer": "^10.1.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  }
}
