  "workspaces": [
    "../@duely/build",
    "../@duely/core",
    "../@duely/client",
    "../@duely/db",
    "../@duely/lambda",
    "../@duely/react"
  ],
  "dependencies": {
    "@duely/core": "0.4.19",
    "@duely/db": "0.4.16",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@duely/build": "0.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.37",
    "@types/validator": "^13.1.1",
    "cross-env": "^7.0.3",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
  }
