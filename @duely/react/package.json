{
  "name": "@duely/react",
  "version": "0.0.31",
  "description": "UI components used in duely.app",
  "repository": "uoleevi/duely",
  "license": "MIT",
  "author": "UoLeevi",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "files": [
    "dist",
    "preset-duely.js"
  ],
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "build:css": "cross-env NODE_ENV='production' postcss src/styles/tailwind.css -o src/styles.module.css",
    "watch:css": "cross-env postcss -w src/styles/tailwind.css -o src/styles.module.css",
    "prepare": "run-s build",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@tailwindcss/typography": "^0.2.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-plugin-react": "^7.21.5",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.11.0",
    "react-icons": "^3.11.0",
    "react-is": "^17.0.1",
    "react-scripts": "4.0.0",
    "tailwindcss": "^1.9.6"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-hook-form": "^6.10.1",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {}
}
