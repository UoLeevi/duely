{
  "name": "@duely/react",
  "version": "0.2.17",
  "description": "UI components used in duely.app",
  "repository": "github:uoleevi/duely",
  "license": "MIT",
  "author": "Leevi Uotinen",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "preset-duely.js"
  ],
  "scripts": {
    "build": "webpack",
    "build-storybook": "build-storybook",
    "build:css": "cross-env NODE_ENV='production' postcss src/styles/tailwind.css -o src/styles.module.css",
    "storybook": "start-storybook -p 6006",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "watch:css": "cross-env postcss -w src/styles/tailwind.css -o src/styles.module.css"
  },
  "dependencies": {
    "@duely/core": "0.0.18"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@headlessui/react": "^0.2.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@tailwindcss/typography": "^0.3.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
