    "@storybook/react": "^6.1.9",
    "@tailwindcss/typography": "^0.3.1",
    "@types/react-router-dom": "^5.1.6",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
