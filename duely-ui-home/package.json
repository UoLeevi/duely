{
  "name": "duely-ui-home",
  "version": "0.9.16",
  "private": true,
  "scripts": {
    "build": "NODE_ENV='production' postcss node_modules/@duely/react/tailwind.css -o public/styles/index.css && react-scripts build",
    "build:css": "NODE_ENV='production' postcss node_modules/@duely/react/tailwind.css -o public/styles/index.css",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "watch:css": "postcss -w node_modules/@duely/react/tailwind.css -o public/styles/index.css"
  },
  "browserslist": {
    "production": [
      ">2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@duely/client": "0.0.102",
    "@duely/core": "0.0.62",
    "@duely/react": "^0.2.71",
    "@headlessui/react": "^0.2.0",
    "@tailwindcss/typography": "^0.3.1",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/react-dom": "^17.0.3",
    "graphql": "^15.5.0",
    "immer": "^9.0.1",
    "markdown-to-jsx": "^7.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.5",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^9.8.6",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.4"
  }
}
