{
  "name": "duely-ui-home",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV='production' postcss src/styles/tailwind.css -o public/styles/index.css && react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "build:css": "postcss src/styles/tailwind.css -o public/styles/index.css",
    "test": "react-scripts test",
    "watch:css": "postcss -w src/styles/tailwind.css -o public/styles/index.css"
  },
  "browserslist": {
    "production": [
      ">2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@duely/client": "0.0.17",
    "@duely/react": "0.0.16",
    "@headlessui/react": "^0.2.0",
    "@tailwindcss/typography": "^0.2.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/user-event": "^12.2.0",
    "graphql": "^15.4.0",
    "immer": "^7.0.14",
    "markdown-to-jsx": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.11.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "tailwindcss": "^1.9.6",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "postcss-cli": "^8.2.0"
  }
}
