    "analyze": "npx webpack-bundle-analyzer stats.json"
  },
  "workspaces": [
    "../@duely/build",
    "../@duely/core",
    "../@duely/client",
    "../@duely/db",
    "../@duely/lambda",
    "../@duely/react",
    "../@duely/stripe",
    "../@duely/util"
  ],
  "dependencies": {
    "@duely/core": "^0.4.34",
    "@duely/db": "^0.4.23",
    "@duely/lambda": "0.4.1",
    "@duely/stripe": "^0.4.10",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@duely/build": "^0.4.7",
    "@duely/util": "^0.4.7",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.37",
    "@types/validator": "^13.1.1",
    "cross-env": "^7.0.3",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
