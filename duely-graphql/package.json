{
  "name": "duely-graphql",
  "version": "0.105.58",
  "repository": "github:uoleevi/duely",
  "license": "MIT",
  "author": "Leevi Uotinen",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "cross-env NODE_ENV='production' webpack",
    "build+profile": "cross-env NODE_ENV='production' webpack --profile --json > stats.json",
    "watch": "webpack --watch",
    "analyze": "npx webpack-bundle-analyzer stats.json",
    "about:genschema-stripe": "echo 'After using the script, convert field names to snake case using find regex $\\s+\\w+[(:] with Transform to Snake Case command from vscode command palette'",
    "genschema-stripe": "node node_modules\\openapi-to-graphql-cli\\dist\\index.js generated-schemas\\stripe\\spec3.yaml --save generated-schemas\\stripe\\schema-stripe.graphql --no-viewer --no-equivalentToMessages"
  },
  "workspaces": [
    "../@duely/build",
    "../@duely/client",
    "../@duely/db",
    "../@duely/email",
    "../@duely/lambda",
    "../@duely/react",
    "../@duely/stripe",
    "../@duely/util"
  ],
  "dependencies": {
    "@duely/db": "^0.4.20",
    "@duely/email": "^0.4.9",
    "@duely/stripe": "^0.4.9",
    "@duely/util": "^0.4.7",
    "@graphql-tools/schema": "^7.1.3",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-parse-resolve-info": "^4.11.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-subscriptions": "^1.2.1",
    "graphql-tag": "^2.11.0",
    "markdown-it": "^12.0.6",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@duely/build": "^0.4.7",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/markdown-it": "^12.0.1",
    "@types/node": "^14.14.41",
    "@types/validator": "^13.1.3",
    "cross-env": "^7.0.3",
    "jest": "^27.0.6",
    "openapi-to-graphql-cli": "^2.5.1",
    "ts-jest": "^27.0.4",
    "webpack": "^5.35.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3"
  }
}
