{
  "name": "duely-graphql",
  "version": "0.103.7",
  "keywords": [],
  "repository": "github:uoleevi/duely",
  "license": "ISC",
  "author": "",
  "main": "src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src -d dist --source-maps --copy-files --no-copy-ignored --extensions '.js,.ts'",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-async-to-generator",
      "@babel/plugin-transform-regenerator",
      [
        "@babel/plugin-transform-runtime",
        {
          "helpers": true,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "rules": {
      "semi": "error",
      "no-unused-vars": "off"
    },
    "env": {
      "node": true,
      "es6": true
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "apollo-server-express": "^2.19.0",
    "base64url": "^3.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "graphql": "^14.7.0",
    "graphql-parse-resolve-info": "^4.10.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.8",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^12.0.2",
    "pg": "^8.5.1",
    "stripe": "^8.127.0",
    "validator": "^13.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "eslint": "^7.14.0"
  }
}
